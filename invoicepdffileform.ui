    background-color: darkgray; /* Darker gray on press */
}

/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical, QScrollBar::sub-line:horizontal {
    border: none;
    background-color: rgba(255, 255, 255, 0);
