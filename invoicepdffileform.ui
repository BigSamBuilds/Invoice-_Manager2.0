    width: 14px;
    margin: 15px 0 15px 0;
    border-radius: 0px;
}

/* HANDLE BAR VERTICAL */
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: gray; /* Gray handle bar */
    min-height: 30px;
